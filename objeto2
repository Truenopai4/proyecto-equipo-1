<?php

// Declaración de la clase Urvan
class Urvan {
    // Propiedades que representan las características del automóvil Urvan
    public $marca; // string
    public $modelo; // string
    public $ano; // int (año de fabricación)
    public $color; // string
    public $transmision; // string
    public $motor; // string
    public $cilindros; // int
    public $potencia; // double
    public $combustible; // string
    public $asientos; // int
    public $puertas; // int
    public $capacidad; // int (capacidad de pasajeros de la combi)
    public $aireAcondicionado; // bool (presencia de aire acondicionado)
    public $dimensiones; // string
    public $precio; // double
    public $seguridad; // string
    public $tecnologia; // string

    // Constructor de la clase Urvan que inicializa las propiedades con valores específicos
    public function __construct($marca, $modelo, $ano, $color, $transmision, $motor, $cilindros, $potencia, $combustible, $asientos, $puertas, $capacidad, $aireAcondicionado, $dimensiones, $precio, $seguridad, $tecnologia) {
        $this->marca = $marca;
        $this->modelo = $modelo;
        $this->ano = $ano;
        $this->color = $color;
        $this->transmision = $transmision;
        $this->motor = $motor;
        $this->cilindros = $cilindros;
        $this->potencia = $potencia;
        $this->combustible = $combustible;
        $this->asientos = $asientos;
        $this->puertas = $puertas;
        $this->capacidad = $capacidad;
        $this->aireAcondicionado = $aireAcondicionado;
        $this->dimensiones = $dimensiones;
        $this->precio = $precio;
        $this->seguridad = $seguridad;
        $this->tecnologia = $tecnologia;
    }
}

// Crear un objeto Urvan con valores específicos para sus propiedades
$urvan = new Urvan("Nissan", "Urvan", 2010, "Blanco", "Manual", "2.5L", 4, 140.5, "Diesel", 10, 4, 15, true, "4.9m x 1.9m x 2.2m", 200000.50, "Básica", "Básica");

// Acceder a las propiedades del objeto e imprimir su valor
echo "Marca: " . $urvan->marca . "\n";
echo "Modelo: " . $urvan->modelo . "\n";
echo "Año: " . $urvan->ano . "\n";
echo "Color: " . $urvan->color . "\n";
// y así sucesivamente para todas las propiedades del objeto
?>
